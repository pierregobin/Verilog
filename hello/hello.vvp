#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55ee728ad660 .scope module, "hello" "hello" 2 3;
 .timescale 0 0;
    .scope S_0x55ee728ad660;
T_0 ;
    %vpi_call 2 6 "$display", "Hello, World" {0 0 0};
    %vpi_call 2 7 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "hello.v";
